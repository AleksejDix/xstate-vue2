{
  "name": "xstate-vue2",
  "version": "0.0.0",
  "description": "XState tools for Vue",
  "keywords": [
    "state",
    "machine",
    "statechart",
    "scxml",
    "state",
    "graph",
    "vue",
    "composable"
  ],
  "author": "Christopher Shank <chris.shank.23@gmail.com>",
  "homepage": "https://github.com/ChrisShank/xstate-vue2#readme",
  "license": "MIT",
  "main": "./dist/xstate-vue2.umd.js",
  "module": "./dist/xstate-vue2.es.js",
  "exports": {
    ".": {
      "import": "./dist/xstate-vue2.es.js",
      "require": "./dist/xstate-vue2.umd.js"
    }
  },
  "types": "types/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist",
    "types"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/ChrisShank/xstate-vue2.git"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "test": "jest"
  },
  "bugs": {
    "url": "https://github.com/ChrisShank/xstate-vue2/issues"
  },
  "peerDependencies": {
    "@vue/composition-api": "^1.0.0-rc.1",
    "@xstate/fsm": "^1.0.0",
    "vue": "^2.6.0",
    "xstate": "^4.7.8"
  },
  "peerDependenciesMeta": {
    "@xstate/fsm": {
      "optional": true
    },
    "xstate": {
      "optional": true
    }
  },
  "devDependencies": {
    "@testing-library/vue": "^5.0.0",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.3.0",
    "@vue/composition-api": "^1.0.0-rc.1",
    "@xstate/fsm": "^1.0.0",
    "babel-core": "^6.26.3",
    "husky": "^6.0.0",
    "jest": "^26.4.2",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.0",
    "ts-jest": "^26.4.0",
    "typescript": "^4.1.2",
    "vite": "^2.3.0",
    "vite-plugin-vue2": "^1.5.1",
    "vue": "^2.6.0",
    "vue-jest": "^3.0.0",
    "vue-template-compiler": "^2.6.12",
    "xstate": "^4.7.8"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,json,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
